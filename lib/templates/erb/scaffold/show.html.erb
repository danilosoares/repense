<div class="col-lg-12">
  <a href="/<%= plural_table_name %>">
      <button type="button" class="btn btn-default">Voltar</button>
  </a>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    	<% attributes.reject(&:password_digest?).each do |attribute| -%>
	      <div class="form-group">
          <label for="number" class="control-label col-lg-3"><%= attribute.human_name %></label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%%= @<%= singular_table_name %>.<%= attribute.name %> %></label>
          </div>
	      </div>
	    <% end -%>
    </div>
  </section>
</div>